{
  "Description": "Primary stack for automl-highlight-detection-data",
  "Parameters": {
    "ServiceDockerImageUrl": {
      "Description": "URL of the Docker image utilized by the ECS service.",
      "Type": "String"
    }
  },
  "Resources": {
    "Cluster": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "TemplateURL": "https://automl-highlight-detection-data-deployment.s3.amazonaws.com/cluster.json",
        "Parameters": {
          "EnvironmentName": {
            "Ref": "AWS::StackName"
          }
        }
      }
    },
    "Service": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "TemplateURL": "https://automl-highlight-detection-data-deployment.s3.amazonaws.com/service.json",
        "Parameters": {
          "EnvironmentName": {
            "Ref": "AWS::StackName"
          },
          "Cluster": {
            "Fn::GetAtt": ["Cluster", "Outputs.Cluster"]
          }
        }
      }
    }
  }
}
