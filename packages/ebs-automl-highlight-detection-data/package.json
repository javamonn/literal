    "@google-cloud/storage": "^4.3.1",
    "@types/ramda": "^0.26.43",
    "@types/request": "^2.48.4",
    "@types/uuid": "^7.0.0",
    "appium": "^1.17.0",
    "p-limit": "^2.2.2",
    "p-retry": "^4.2.0",
    "p-series": "^2.1.0",
    "ramda": "^0.27.0",
    "uuid": "^7.0.1",
    "webdriverio": "^5.22.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^13.7.7",
    "concurrently": "^5.3.0",
    "typescript": "^3.8.3"
  }
