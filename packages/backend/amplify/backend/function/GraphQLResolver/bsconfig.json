{
  "name": "GraphQLResolver",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "decco",
    "@trashed/bs-aws-amplify",
    "bs-fetch"
  ],
  "ppx-flags": [
    "decco/ppx",
    ["@baransu/graphql_ppx_re/ppx", "-schema ./graphql-schema.json"]
  ],
  "warnings": {
    "error": "+101"
  },
  "namespace": true,
  "refmt": 3
}
