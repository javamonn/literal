{
	"auth": {
		"literalAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"api": {
		"literal": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authliteralAuth"
						}
					}
				}
			}
		}
	},
	"function": {
		"GraphQLResolver": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "literalAuth",
					"attributes": [
						"UserPoolId"
					]
				},
				{
					"category": "api",
					"resourceName": "literal",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				},
				{
					"category": "storage",
					"resourceName": "literalStorage",
					"attributes": [
						"BucketName"
					]
				}
			]
		},
		"CloudfrontOriginRequest": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	},
	"storage": {
		"literalStorage": {
			"service": "S3",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "CloudfrontOriginRequest",
					"attributes": [
						"VersionArn"
					]
				}
			]
		}
	}
}