{
	"auth": {
		"literalAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"api": {
		"literal": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "AWS_IAM"
					}
				}
			}
		}
	},
	"function": {
		"GraphQLResolver": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "literalAuth",
					"attributes": [
						"UserPoolId"
					]
				},
				{
					"category": "api",
					"resourceName": "literal",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				},
				{
					"category": "storage",
					"resourceName": "literalStorage",
					"attributes": [
						"BucketName"
					]
				}
			]
		},
		"CloudfrontOriginRequest": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	},
	"storage": {
		"literalStorage": {
			"service": "S3",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	}
}