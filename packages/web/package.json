  "scripts": {
    "start": "npm run bsb:clean && npm run bsb:build && concurrently -k -n \"bsb,next\" \"npm run bsb:start\" \"npm run next:start\"",
    "next:start": "next",
    "next:build": "next build",
    "bsb:build": "bsb -make-world",
    "bsb:start": "bsb -make-world -w",
    "bsb:clean": "bsb -clean-world && ./scripts/bsb-post-clean.sh",
    "amplify:graphql-sync-schema": "./scripts/graphql-sync-schema.sh && npm run bsb:clean",
    "amplify:production:pull": "amplify env pull --restore && amplify env checkout production && amplify api gql-compile"
  },
