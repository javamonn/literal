{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run bsb:clean && npm run bsb:build && concurrently -k -n \"bsb,next\" \"npm run bsb:start\" \"npm run next:dev\"",
    "build:production": "export NODE_ENV=production && npm run bsb:clean && npm run bsb:build && next build && next export",
    "next:dev": "next",
    "next:start": "NODE_ENV=production next start",
    "next:build": "NODE_ENV=production next build",
    "bsb:build": "bsb -make-world",
    "bsb:start": "bsb -make-world -w",
    "bsb:clean": "bsb -clean-world && ./scripts/bsb-post-clean.sh",
    "amplify:graphql-sync-schema": "./scripts/graphql-sync-schema.sh && ./scripts/generate-fragment-types.js && npm run bsb:clean",
    "amplify:production:pull": "amplify env pull --restore && amplify env checkout production && amplify api gql-compile"
  },
